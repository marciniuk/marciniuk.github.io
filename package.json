{
  "name": "marcini.uk",
  "type": "module",
  "version": "3.2.0",
  "description": "Strona powstała pod wpływem kofeiny oraz zbyt dużej ilości wolnego czasu.",
  "main": "index.html",
  "scripts": {
    "build": "npx @tailwindcss/cli -i ./input.css -o ./assets/css/main.css --watch",
    "build:prod": "npx @tailwindcss/cli -i ./input.css -o ./assets/css/main.css --minify && node prettier-summary.js && node generate-sitemap.js",
    "head": "node assets/replace/replace-heads.js",
    "footer": "node assets/replace/replace-footers.js",
    "header": "node assets/replace/replace-headers.js",
    "full": "node assets/replace/html-structure.js",
    "format": "node prettier-summary.js",
    "cliF": "npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marciniuk/marciniuk.github.io.git"
  },
  "author": "Marcini.uk",
  "license": "ISC",
  "homepage": "https://marcini.uk",
  "devDependencies": {
    "sitemap": "^9.0.0",
    "tailwind-rounded-out": "^2.0.0",
    "tailwind-scrollbar": "^4.0.0",
    "tailwindcss": ">=4.0.0",
    "xml-formatter": "^3.6.7"
  },
  "dependencies": {
    "prettier": "^3.6.2"
  }
}
